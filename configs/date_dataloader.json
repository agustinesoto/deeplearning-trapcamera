{
  "exp": {
    "name": "conv_mnist_from_config"
  },
  "data_loader": {
    "name": "date_loader.ConvMnistDataLoader"
  },
   "data_input":{
       "parent_dir":"data",
       "camera_dir":"I23",
       "width_shape":224,
       "height_shape":224
   },
  "model":{
    "name": "conv_mnist_model.DateDataLoader",
    "optimizer": "adam"
  },
  "trainer":{
    "name": "simple_mnist_trainer.SimpleMnistModelTrainer",
    "num_epochs": 100,
    "batch_size": 32,
